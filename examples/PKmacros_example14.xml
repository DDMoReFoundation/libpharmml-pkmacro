<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/pharmml/0.6/PharmML http://www.pharmml.org/pharmml/0.6/PharmML"
    xmlns:ct="http://www.pharmml.org/pharmml/0.6/CommonTypes"
    xmlns:math="http://www.pharmml.org/pharmml/0.6/Maths" 
    xmlns:ds="http://www.pharmml.org/pharmml/0.6/Dataset"
    implementedBy="MJS" writtenVersion="0.6" id="i1">
    
    <!-- MODEL NAME -->
    <ct:Name>Advan1</ct:Name>
    
    <ct:Description>Advan1/Trans1 - model with PK macros</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="t"/>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/pharmml/0.6/ModelDefinition">
        
        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="modelVar" type="parameterVariability"> 
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <VariabilityModel blkId="obsErr" type="residualError"> 
            <Level symbId="residual"/>
        </VariabilityModel>
                
        <ParameterModel blkId="pm1">
            <SimpleParameter symbId="V"/>
            <SimpleParameter symbId="CL"/>
            <SimpleParameter symbId="k">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="divide">
                            <ct:SymbRef symbIdRef="CL"/>
                            <ct:SymbRef symbIdRef="V"/>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="a"/>
        </ParameterModel>
        
        <!--
            compartment(cmt=1, amount=Ac, volume=V) 
            iv(adm=1, cmt=1)
            elimination(cmt=1, k)
            Cc = Ac/V
        -->
 
        <StructuralModel blkId="sm1">
            <SimpleParameter symbId="Mtt"/>
            <SimpleParameter symbId="Ktr"/>
            <SimpleParameter symbId="ka"/>
            <ct:Variable symbolType="real" symbId="Al"/>
            <ct:Variable symbolType="real" symbId="Ac"/>
            <ct:Variable symbolType="real" symbId="C1"/>
            <ct:Variable symbolType="real" symbId="C2"/>
            
            <PKmacros>
                <Compartment>
                    <Value argument="cmt">
                        <ct:Int>1</ct:Int>
                    </Value>
                    <Value argument="amount">
                        <ct:SymbRef symbIdRef="Al"/>
                    </Value>
                    <Value argument="volume">
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="V1"/>
                    </Value>
                    <Value argument="concentration">
                        <ct:SymbRef symbIdRef="C1"/>
                    </Value>
                </Compartment>
                
                <Compartment>
                    <Value argument="cmt">
                        <ct:Int>2</ct:Int>
                    </Value>
                    <Value argument="amount">
                        <ct:SymbRef symbIdRef="Ac"/>
                    </Value>
                    <Value argument="volume">
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="V2"/>
                    </Value>
                    <Value argument="concentration">
                        <ct:SymbRef symbIdRef="C2"/>
                    </Value>
                </Compartment>
                
                <Oral>
                    <Value argument="adm">
                        <ct:Int>1</ct:Int>
                    </Value>
                    <Value argument="cmt">
                        <ct:Int>1</ct:Int>
                    </Value>
                    <Value argument="ka">
                        <ct:SymbRef symbIdRef="ka1"/>
                    </Value>
                    <Value argument="p">
                        <ct:SymbRef symbIdRef="F1"/>
                    </Value>
                </Oral>
                
                <Oral>
                    <Value argument="adm">
                        <ct:Int>2</ct:Int>
                    </Value>
                    <Value argument="cmt">
                        <ct:Int>2</ct:Int>
                    </Value>
                    <Value argument="ka">
                        <ct:SymbRef symbIdRef="ka2"/>
                    </Value>
                    <Value argument="p">
                        <ct:SymbRef symbIdRef="F2"/>
                    </Value>
                </Oral>
                
                <IV>
                    <Value argument="adm">
                        <ct:Int>3</ct:Int>
                    </Value>
                    <Value argument="cmt">
                        <ct:Int>2</ct:Int>
                    </Value>
                </IV>
                
                <Transfer>
                    <Value argument="from">
                        <ct:Int>1</ct:Int>
                    </Value>
                    <Value argument="to">
                        <ct:Int>2</ct:Int>
                    </Value>
                    <Value argument="kt">
                        <ct:SymbRef symbIdRef="kl"/>
                    </Value>
                </Transfer>
                
                <Elimination>
                    <Value argument="cmt">
                        <ct:Int>1</ct:Int>
                    </Value>
                    <Value argument="k">
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="k1"/>
                    </Value>
                </Elimination>
                
                <Elimination>
                    <Value argument="cmt">
                        <ct:Int>2</ct:Int>
                    </Value>
                    <Value argument="k">
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="k2"/>
                    </Value>
                </Elimination>
                
            </PKmacros>
        </StructuralModel>


        <ObservationModel blkId="om1">
            <ContinuousData>
                <RandomVariable symbId="eta">
                    <ct:VariabilityReference>
                        <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                    </ct:VariabilityReference>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                        <mean><rVal>0</rVal></mean>
                        <stddev><prVal>1</prVal></stddev>
                    </NormalDistribution>
                </RandomVariable>
                <Standard symbId="Cc_obs">
                    <Output>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="Cc"/>
                    </Output>
                    <ErrorModel>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="a"/>
                        </ct:Assign>
                    </ErrorModel>
                    <ResidualError>
                        <ct:SymbRef symbIdRef="eta"/>
                    </ResidualError>
                </Standard>
            </ContinuousData>
        </ObservationModel>
    </ModelDefinition>


    <ModellingSteps xmlns="http://www.pharmml.org/pharmml/0.6/ModellingSteps">
        
        <ExternalDataSet toolName="Monolix" oid="MLXoid">
            
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="t"/>
            </ColumnMapping>
            
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/pharmml/0.6/Dataset" columnIdRef="DV"/>
                <ct:SymbRef blkIdRef="om1" symbIdRef="Cc_obs"/>
            </ColumnMapping>
            
            <ColumnMapping>
                <ds:ColumnRef columnIdRef="ADM"/>
                <ds:TargetMapping blkIdRef="sm1">
                    <ds:Map dataSymbol="1" admNumber="1"/>
                </ds:TargetMapping>
            </ColumnMapping>             
            
            <DataSet xmlns="http://www.pharmml.org/pharmml/0.6/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                    <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    <Column columnId="AMT" columnType="dose" valueType="real" columnNum="4"/>
                    <Column columnId="ADM" columnType="adm" valueType="int" columnNum="5"/> 
                </Definition>
                <ExternalFile oid="importMLX">
                    <path>example1.csv</path>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ExternalFile>
            </DataSet>
        </ExternalDataSet>
        
        
        <!-- ESTIMATION STEP -->
        <EstimationStep oid="estTask1">
            
            <ExternalDataSetReference>
                <ct:OidRef oidRef="MLXoid"/>
            </ExternalDataSetReference>
            
            <ParametersToEstimate>
                
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="a"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="V"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- omitted other parameters -->
            </ParametersToEstimate>
            
            
            <Operation order="1" opType="estPop"/>
            <Operation order="2" opType="estIndiv"/>
        </EstimationStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="estTask1"/>
            </Step>
        </StepDependencies>
        
    </ModellingSteps>
</PharmML>